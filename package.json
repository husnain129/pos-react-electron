{
  "name": "electron-react-tailwind",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "bun run server/index.ts",
    "build:web": "vite build",
    "build:server": "bunx tsc -p tsconfig.server.json",
    "build:desktop": "bun run scripts/build-desktop.ts",
    "start:web": "vite preview",
    "start:desktop": "electron dist-electron/main.js",
    "lint": "bunx @biomejs/biome check --write src",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@electron/rebuild": "^4.0.2",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tanstack/react-query": "^5.90.16",
    "@tanstack/react-table": "^8.21.3",
    "@types/printer": "^0.4.5",
    "axios": "^1.13.2",
    "body-parser": "^2.2.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "happy-dom": "^15.11.7",
    "lucide-react": "^0.441.0",
    "moment": "^2.30.1",
    "node-thermal-printer": "^4.5.0",
    "pg": "^8.16.3",
    "printer": "^0.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.11.0",
    "recharts": "^3.6.0",
    "sweetalert2": "^11.26.17",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/moment": "^2.13.0",
    "@types/node": "^22.19.3",
    "@types/pg": "^8.16.0",
    "@types/react": "^18.3.26",
    "@types/react-dom": "^18.3.7",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-istanbul": "^2.1.9",
    "@vitest/ui": "^2.1.9",
    "autoprefixer": "^10.4.21",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.9.3",
    "vite": "^5.4.21",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "vitest": "^2.1.9"
  },
  "main": "dist-electron/main.js",
  "build": {
    "directories": {
      "output": "release"
    },
    "appId": "com.electron.react.tailwind",
    "mac": {
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "zip"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb",
        "rpm",
        "tar.gz"
      ],
      "category": "Utility"
    }
  }
}
